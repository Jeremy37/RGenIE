citHeader("To cite RGenIE in publications use:")

citEntry(entry = "Article",
  title        = "Identification of functional transcriptional and splicing
regulatory elements using GenIE",
  author       = personList(as.person("Jeremy Schwartzentruber")),
  journal      = "Nucleic Acids Research",
  year         = "2020",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "",

  textVersion  =
  paste("Sarah E. Cooper, Jeremy Schwartzentruber, Erica Bello, Eve L Coomber, Andrew R. Bassett (2020).",
        "Screening for functional transcriptional and splicing regulatory variants with GenIE.",
        "Nucleic Acids Research.",
        "")
)
